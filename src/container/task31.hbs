{{!
Це код container файлу для summary сторінки,
в якому міститься код, котрий буде віддаватися по шляху (PATH) - "/summary"
}}
<main>
  <div>
    <section>

      <div class='row'>
        <div class='col-6'>

          {{#with navigation}}
            <ul>
              {{#each links}}
                <a href={{url}}>{{text}}</a>
              {{/each}}
            </ul>

            {{#with button}}

              <button
                class='btn btn-primary'
              >{{text}}</button>

            {{/with}}
          {{/with}}
        </div>
      </div>
      <hr />
      {{#with header}}
        <h1>{{title}}</h1>
        <p>
          {{description}}
        </p>
        {{#with button}}
          <button
            class='btn btn-primary href{{url}}'
          >{{text}}</button>
        {{/with}}
      {{/with}}
    </section>
  </div>
  <hr />
  <section class='row-gy-5'>
    <div>
    <div class='col-12'>
<div class='col-5'>
      {{#with city}}
<div class="card-header"> <h2>{{title}}</h2></div>
       

        <ul class="list-group list-group-flush">
          {{#each cards}}
            <li class="list-group-item">
              <ul class="card-img-top">
                <img class="card-img-top"
                src="https://picsum.photos/400/200"
                alt="New York">
            </ul>
            <div class="card-body">
             <div class="card-title" >{{title}}</div>
             <p>
                {{description}}
              </p>
             <div>
               <a class="btn btn-primary" class="text"></a>
              </div>
           

          {{/each}}
       

      {{/with}}
      </div>
      </div>
    </div>
  </section>
  </div>
  <br />
  <br />
  <br />
  <div>
    <ul>
      {{#with winner}}
        <li>
          {{title}}
          <ul>
            {{#with table}}
              <li>
                {{#each head}}
                  {{this}}
                {{/each}}
              </li>
              <ul>
                {{#each user}}
                  <li>
                    {{place}}
                    {{name}}
                    {{surname}}
                    {{nickname}}
                  </li>

                {{/each}}
              </ul>
            {{/with}}
          </ul>

        </li>
      {{/with}}
    </ul>

  </div>

  <br />
  <div>
    <ul>
      {{#with gallery}}
        <li>
          {{title}}
          <ul>
            {{#each cards}}
              <li>
                {{name}}
                <ul>
                  {{#each items}}
                    <li>
                      {{this}}</li>
                  {{/each}}
                </ul>
              </li>
            {{/each}}
          </ul>
        </li>

      {{/with}}
    </ul>
  </div>

  <br />
  <div>
    <ul>
      {{#with relaxion}}
        <li>
          {{title}}
          <ul>
            {{#each cards}}
              <li>
                <p>
                  {{header}}</p>
                <p>
                  {{title}}</p>
                <p>
                  {{description}}
                </p>
              </li>
            {{/each}}
          </ul>
        </li>
      {{/with}}
    </ul>
  </div>
  <br />
  <div>
    <ul>
      {{#with rest}}

        <li>
          <ul>
            {{#each cards}}
              <li>
                <p>
                  {{header}}</p>
                <p>
                  {{title}}
                </p>
                <p>
                  {{description}}
                </p>
                <button
                  class='btn btn-primary'
                >{{button}}</button>
                <p>{{footer}}</p>

              </li>

            {{/each}}
          </ul>
        </li>
      {{/with}}
    </ul>
  </div>
  <br />
  <ul>
    {{#with proposition}}

      <li>
        <p>
          {{title}}
        </p>
        <ul>
          {{#each card}}
            <li>
              <p>
                {{header}}
              </p>
              <p>
                {{title}}
              </p>
              <p>
                {{description}}
              </p>
            </li>

          {{/each}}
        </ul>
      </li>
    {{/with}}

  </ul>
  <br />
  <ul>
    {{#with footer}}
      <p>
        {{title}}
      </p>
      <ul>
        {{#each navigation}}
          <li>
            <p>
              {{text}}
            </p>
            {{href}}
          </li>

        {{/each}}
      </ul>
      <br />
      <ul>
        {{#each socialMedia}}
          <li>
            {{name}}</li>

          {{link}}
        {{/each}}
      </ul>
      <br />
      <ul>
        {{#each partners}}
          <li>
            {{name}}
          </li>

          {{link}}
        {{/each}}
      </ul>
    {{/with}}
  </ul>

</main>


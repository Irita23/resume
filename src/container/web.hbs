{{!--
Це код container файлу для summary сторінки,
в якому міститься код, котрий буде віддаватися по шляху (PATH) - "/summary"
--}}


<div>
    <div>
        {{#with web}}

        <ul>
            {{#each languages}}
            <li>
                <h1>{{name}}</h1>
                {{version}}
                <p>{{description}}</p>
            </li>
            <br>
            <div>
                <ul>
                    {{#each elements}}
                    <li>
                        {{name}}
                        <p>
                            {{description}}
                        </p>
                    </li>
                    <div>
                        <ul>
                            {{#each attributes}}
                            <li>
                                {{name}}
                                <p>
                                    {{description}}
                                </p>
                            </li>
                            {{/each}}
                        </ul>
                    </div>
                    {{/each}}
                </ul>
            </div>
            <br>
            <div>
                <ul>
                    {{#each properties}}
                    <li>
                        {{name}}
                        <p>
                            {{description}}
                        </p>
                    </li>
                    <div>
                        <ul>
                            {{#each values}}
                            <li>
                                {{value}}
                                <p>
                                    {{description}}
                                </p>
                            </li>
                            {{/each}}
                        </ul>
                    </div>
                    {{/each}}
                </ul>
            </div>
            <br>
            <div>
                <ul>
                    {{#each functions}}
                    <li>
                        {{name}}
                        <p>
                            {{description}}
                        </p>
                    </li>
                    <div>
                        <ul>
                            {{#each parameters}}
                            <li>
                                {{name}}
                                {{type}}
                                <p>
                                    {{description}}
                                </p>
                            </li>
                            {{/each}}
                        </ul>
                    </div>
                    {{/each}}
                </ul>
            </div>
            {{/each}}
        </ul>
        {{/with}}
    </div>
</div>










{{!--
Цей код містить лише те, що буде виводитись в {{{body}}} в layout/default.hbs
Адже наш layout/default.hbs має вже структуру HTML
--}}